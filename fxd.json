{
    "name": "Freql",
    "outputDirectory": "docs",
    "args": [
        {
            "name": "root",
            "default": "../../",
            "required": true
        },
        {
            "name": "css_root",
            "default": "../../css",
            "required": true
        },
        {
            "name": "js_root",
            "default": "../../js",
            "required": true
        }
    ],
    "metadata": [
        {
            "key": "repo_url",
            "value": "https://github.com/mc738/Freql"
        },
        {
            "key": "home_link",
            "value": "@root"
        },
        {
            "key": "css_root",
            "value": "@css_root"
        },
        {
            "key": "js_root",
            "value": "@js_root"
        }
    ],
    "directories": [
        "css",
        "js",
        "img"
    ],
    "resources": [
        {
            "name": "style.css",
            "outputPath": "css\\style.css"
        },
        {
            "name": "index.js",
            "outputPath": "js\\index.js"
        },
        {
            "name": "prism.js",
            "outputPath": "js\\prism.js"
        },
        {
            "name": "prism.css",
            "outputPath": "css\\prism.css"
        }
    ],
    "sections": [
        {
            "title": "Documents",
            "type": "articles",
            "regexIgnore": "Internal",
            "template": "article",
            "items": [
                "Docs\\Overview.md",
                "Docs\\Setup.md",
                "Docs\\Examples.md"
            ]
        },
        {
            "title": "Freql.Csv",
            "type": "fsharp_code_documents",
            "regexIgnore": "Internal|Extensions|QueryHelpers",
            "template": "fsharp_code_doc",
            "items": [
                "Freql.Csv\\Library.fs"
            ]
        },
        {
            "title": "Freql.MySql",
            "type": "fsharp_code_documents",
            "regexIgnore": "Internal|Extensions|QueryHelpers",
            "template": "fsharp_code_doc",
            "items": [
                "Freql.MySql\\Library.fs"
            ]
        },
        {
            "title": "Freql.Sqlite",
            "type": "fsharp_code_documents",
            "regexIgnore": "Internal|Extensions|QueryHelpers",
            "template": "fsharp_code_doc",
            "items": [
                "Freql.Sqlite\\Library.fs"
            ]
        },
        {
            "title": "Freql.SqlServer",
            "type": "fsharp_code_documents",
            "regexIgnore": "Internal|Extensions|QueryHelpers",
            "template": "fsharp_code_doc",
            "items": [
                "Freql.SqlServer\\Library.fs"
            ]
        },
        {
            "title":  "Reports",
            "type": "reports",
            "regexIgnore": "",
            "template": "reports",
            "items": [
                "project_report"
            ]
        }
    ]
}